{
  "name": "huhwt-xtv",
  "version": "1.0.0",
  "description": "HuHwt webtrees Extensions - Interactive Treeview XT",
  "main": "index.js",
  "keywords": [
    "webtrees",
    "treeview"
  ],
  "author": "huhwt",
  "license": "ISC",
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env process.env.section=devel NODE_ENV=development node_modules/webpack/bin/webpack.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env process.env.section=devel NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dist": "npm run distribution",
    "distribution": "npm run development && cross-env process.env.section=distribution NODE_ENV=production node_modules/webpack/bin/webpack.js --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "file-icon-vectors": "^1.0.0",
    "filemanager-webpack-plugin": "^2.0.5",
    "laravel-mix": "5.0.5",
    "laravel-mix-clean": "^0.1.0",
    "postcss-custom-properties": "^10.0.0",
    "postcss-discard-comments": "^4.0.2",
    "postcss-image-inliner": "^4.0.3",
    "postcss-import": "^12.0.1",
    "postcss-replace": "^1.1.2",
    "postcss-rtl": "^1.7.3",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  }
}
